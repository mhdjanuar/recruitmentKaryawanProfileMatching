		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="MySQL"/>
		<queryString language="SQL">
			<![CDATA[SELECT 
    p.name AS nama_pelanggan,
    ROUND(SUM(
        CASE 
            WHEN c.type = 'benefit' THEN (sc.jumlah_bobot / pembagi_table.pembagi) * (c.bobot / total_weight.total)
            WHEN c.type = 'cost' THEN (pembagi_table.pembagi / sc.jumlah_bobot) * (c.bobot / total_weight.total)
        END
    ), 4) AS total_nilai,
    RANK() OVER (ORDER BY 
        SUM(
            CASE 
                WHEN c.type = 'benefit' THEN (sc.jumlah_bobot / pembagi_table.pembagi) * (c.bobot / total_weight.total)
                WHEN c.type = 'cost' THEN (pembagi_table.pembagi / sc.jumlah_bobot) * (c.bobot / total_weight.total)
            END
        ) DESC
    ) AS peringkat
FROM saw_kinerja_karyawan_biznet.alternatif AS a
JOIN saw_kinerja_karyawan_biznet.employees AS p ON a.id_employee = p.id
JOIN saw_kinerja_karyawan_biznet.sub_criteria AS sc ON a.id_sub_kreteria = sc.id
JOIN saw_kinerja_karyawan_biznet.criteria AS c ON sc.id_kreteria = c.id

-- Subquery untuk menghitung nilai pembagi tiap kriteria
JOIN (
    SELECT 
        c.id AS id_kriteria,
        CASE 
            WHEN c.type = 'benefit' THEN MAX(sc.jumlah_bobot)
            WHEN c.type = 'cost' THEN MIN(sc.jumlah_bobot)
        END AS pembagi
    FROM saw_kinerja_karyawan_biznet.alternatif AS a
    JOIN saw_kinerja_karyawan_biznet.sub_criteria AS sc ON a.id_sub_kreteria = sc.id
    JOIN saw_kinerja_karyawan_biznet.criteria AS c ON sc.id_kreteria = c.id
    GROUP BY c.id, c.type
) AS pembagi_table ON pembagi_table.id_kriteria = c.id

-- Subquery untuk total bobot
JOIN (
    SELECT SUM(bobot) AS total FROM saw_kinerja_karyawan_biznet.criteria
) AS total_weight

GROUP BY p.name
ORDER BY total_nilai DESC;]]>
		</queryString>
